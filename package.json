{
  "name": "lockersv3-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "dp": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "client": "ts-node renderGraphClientConfig.ts && yarn run graphclient build",
    "graphiql": "yarn workspaces run graphiql",
    "build:config": "ts-node renderGraphClientConfig.ts"
  },
  "workspaces": [
    "src",
    "src/*"
  ],
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@internationalized/date": "^3.2.0",
    "@nivo/core": "^0.83.0",
    "@nivo/pie": "^0.83.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@react-aria/aria-modal-polyfill": "^3.7.1",
    "@reown/appkit": "^1.6.8",
    "@reown/appkit-adapter-wagmi": "^1.6.8",
    "@safe-global/safe-apps-sdk": "^9.1.0",
    "@safe-global/safe-apps-web3modal": "^23.0.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-table": "^8.9.3",
    "@types/progressbar.js": "^1.1.7",
    "class-variance-authority": "^0.7.0",
    "client-only": "^0.0.1",
    "color.js": "1.2.0",
    "d3-color": "3.1.0",
    "date-fns": "^3.3.1",
    "embla-carousel-react": "^8.3.0",
    "extract-colors": "4.0.2",
    "immer": "^10.0.3",
    "js-cookie": "^3.0.5",
    "jsbi": "^4.3.0",
    "lucide-react": "^0.488.0",
    "luxon": "^3.4.3",
    "mersenne-twister": "1.1.0",
    "next": "14.2.4",
    "progressbar.js": "^1.1.1",
    "react": "18.2.0",
    "react-aria-components": "^1.0.1",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.6",
    "react-icons": "^4.8.0",
    "react-intersection-observer": "^9.5.2",
    "react-resizable-panels": "^2.1.2",
    "react-spinners": "^0.14.1",
    "react-stately": "^3.23.0",
    "react-toastify": "^10.0.3",
    "react-tooltip": "^5.26.3",
    "recharts": "^2.11.0",
    "rooks": "^7.14.1",
    "server-only": "^0.0.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.1.0",
    "tailwind-variants": "^0.1.18",
    "tailwindcss-animate": "^1.0.7",
    "uniqolor": "1.1.1",
    "uuid": "^9.0.1",
    "viem": "^2.34.0",
    "wagmi": "^2.16.4",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.1",
    "zustand": "^4.4.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.9",
    "@types/luxon": "^3.3.2",
    "@types/mersenne-twister": "^1.1.7",
    "@types/node": "20.8.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.4",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "10.4.14",
    "clsx": "^2.0.0",
    "eruda": "^3.0.1",
    "graphql": "^16.9.0",
    "lodash-es": "^4.17.21",
    "mustache": "^4.2.0",
    "pino-pretty": "^10.2.3",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "sass": "1.69.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "bracketSameLine": true,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "jsxBracketSameLine": false,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrder": [
      "^react$",
      "^clsx$",
      "^viem$",
      "wagmi",
      "^@core/(.*)$",
      "^@server/(.*)$",
      "^@modules/(.*)$",
      "^@components/(.*)$",
      "^@images/(.*)$",
      "^@src/(.*)$",
      "^@/src/(.*)$",
      "^[../]",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ],
    "importOrderTypeScriptVersion": "5.0.0"
  }
}
